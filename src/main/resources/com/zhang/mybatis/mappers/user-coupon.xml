<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhang.module.login.loginDao">

	<resultMap id="baseMap2" type="com.zhang.module.bean.CouponPo">
		<result column="id" property="id" />
		<result column="userid" property="userid" />
		<result column="coupon" property="coupon" />
	</resultMap>

	<update id="update2" parameterType="com.zhang.module.bean.CouponPo">
		UPDATE
		user_coupon
		SET
		userid = #{userid},
		coupon = #{coupon}
		WHERE id =#{id} 
	</update>
</mapper>